dataset "./data.csv"

model GradientBoostingClassifier {
    backend = sklearn
    n_estimators = 100
    learning_rate = 0.1
    max_depth = 3
    random_state = 42
}

model AdaBoostClassifier {
    backend = sklearn
    n_estimators = 50
    learning_rate = 1.0
    random_state = 42
}

model MLPClassifier {
    backend = sklearn
    hidden_layer_sizes = 100
    activation = "relu"
    solver = "adam"
    max_iter = 500
    random_state = 42
}
