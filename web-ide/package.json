{
  "name": "mlc-web-ide",
  "version": "1.0.0",
  "description": "Web-based IDE for MLC Compiler",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "node backend/server.js",
    "install:all": "npm install && cd frontend && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "archiver": "^6.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
